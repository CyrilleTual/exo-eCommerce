<h1>ici on regarde les Produits</h1>

<div class="wrapper">
    <% datas.forEach(data=> { %>

        <article class="card">
            <p>Dans la cat√©gorie : <%= data.category %>
            </p>
            <h3>
                <%= data.title %>
            </h3>
            <img src="/<%= data.url_img %>" alt="<%= data.title %>">
            <p>
                <%= data.description%>
            </p>
            <p>Vendeur: <%= data.seller %>  <span>Prix : <%= data.price %> Euros </span>
            </p>

            <a href="/admin/modifyProduct/<%= data.id %>">Modify</a><a href="/admin/removeProduct/<%= data.id%>">Remove</a>
            
        </article>


        <% }) %>

        
       
</div>